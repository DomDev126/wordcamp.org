{
	"name": "wcpt",
	"version": "0.0.1",
	"license": "GPL-2.0+",
	"dependencies": {
		"@wordpress/element": "4.0.4",
		"@wordpress/scripts": "19.2.3",
		"prop-types": "15.8.1"
	},
	"eslintConfig": {
		"extends": "../../../../.eslintrc.js",
		"globals": {
			"wpcApplicationTracker": "true"
		}
	},
	"prettier": "../../../../.prettierrc.js",
	"stylelint": {
		"extends": "../../../../.stylelintrc"
	},
	"scripts": {
		"start": "wp-scripts start",
		"build": "wp-scripts build",
		"lint:js": "wp-scripts lint-js javascript/tracker",
		"format:js": "wp-scripts format javascript/tracker --config=../../../../.prettierrc.js",
		"lint:css": "wp-scripts lint-style javascript/tracker/**/*.scss css/**/*.css"
	}
}
